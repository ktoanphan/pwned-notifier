{
  "name": "Pwned Notifier",
  "description": "Check if the website being visited had a data breach with a single click!",
  "version": "1.1",
  "manifest_version": 3,
  "author": "Toan Phan",

  "permissions": ["activeTab", "tabs"],

  "action": {
    "default_popup": "index.html",
    "default_title": "Pwned Notifier"
  },

  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo512.png"
  }
}